{
  "compilerOptions":{
    /*Language and Environment*/
    "target":"es2020",

    /* Modules */
    "module":"commonjs",
    "moduleResolution":"node",
    "sourceMap": true,
    "outDir": "./dist",
    //"rootDirs": [],

    /* Interop Constraints */
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    /* Type Checking */
    "strict": true,

    /* Completeness */
    "skipLibCheck": true

    /* Emit */
    //"outFile": "./",
    //"outDir": "./",
  },
  "lib": ["es2015"]
}
